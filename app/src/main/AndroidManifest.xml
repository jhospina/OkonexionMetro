<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jhon.okonexionmetro" >

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MenuPrincipal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NoticiasActivity"
            android:label="@string/title_activity_noticias" >
        </activity>
        <activity
            android:name=".VerNoticiaActivity"
            android:label="@string/title_activity_ver_noticia" >
        </activity>
        <activity
            android:name=".InstitucionalActivity"
            android:label="@string/title_activity_institucional" >
        </activity>
        <activity
            android:name=".VerInstitucionalActivity"
            android:label="@string/title_activity_ver_institucional" >
        </activity>
    </application>

</manifest>
